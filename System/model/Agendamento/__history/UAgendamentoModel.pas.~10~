unit UAgendamentoModel;

interface

uses
  System.JSON, System.Classes, CAtribEntity, UBaseModel;

type
  [TableName('agenda')]
  [Route('agenda')]
  TAgendamentoModel = class(TBaseModel)
  private
    Fcod_medico: string;
    Fcod_paciente: string;
    Fmemo_mtvCancel: string;
    FstrDateTime: string;
    procedure SetFcod_medico(const Value: string);
    procedure SetFcod_paciente(const Value: string);
    procedure SetFmodelo_doc(const Value: Integer);
    procedure SetFnatureza_operacao(const Value: Integer);
    procedure SetFserie(const Value: string);
    procedure SetFsubserie(const Value: string);
  public
    [FieldName('mensagem')]
    property mensagem: string read Fmensagem  write SetFmensagem;
    [FieldName('modelo_doc')]
    property modelo_doc: Integer read Fmodelo_doc  write SetFmodelo_doc;
    [FieldName('natureza_operacao')]
    property natureza_operacao: Integer read Fnatureza_operacao  write SetFnatureza_operacao;
    [FieldName('empresa')]
    property empresa: Integer  read Fempresa  write SetFempresa;
    [FieldName('serie')]
    property serie: string read Fserie  write SetFserie;
    [FieldName('subserie')]
    property subserie: string read Fsubserie  write SetFsubserie;
    [FieldName('id')]
    property id: integer read Fid  write SetFid;
  end;

implementation

{ TAgendamentoModel }

